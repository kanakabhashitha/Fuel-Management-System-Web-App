<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         	
		<servlet>
		<servlet-name>updatefs</servlet-name>
		<servlet-class>com.ServiceStation.servlet.UpdateFuelStockServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updatefs</servlet-name>
		<url-pattern>/updatefs</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>insertfs</servlet-name>
		<servlet-class>com.ServiceStation.servlet.InsertFuelStockServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insertfs</servlet-name>
		<url-pattern>/insertfs</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>searchrq</servlet-name>
		<servlet-class>com.ServiceStation.servlet.SearchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>searchrq</servlet-name>
		<url-pattern>/searchrq</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>order</servlet-name>
		<servlet-class>com.ServiceStation.servlet.OrderServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>order</servlet-name>
		<url-pattern>/order</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>getfs</servlet-name>
		<servlet-class>com.ServiceStation.servlet.GetFuelStockServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getfs</servlet-name>
		<url-pattern>/getfs</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>signup</servlet-name>
		<servlet-class>com.ServiceStation.servlet.SignUpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>signup</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>

</web-app>